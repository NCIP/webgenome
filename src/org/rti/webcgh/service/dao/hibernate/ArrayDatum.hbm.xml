<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping>

	<class name="org.rti.webcgh.domain.ArrayDatum" table="ARRAY_DATUM">
	
		<id name="id" column="ID">
		    <generator class="increment"/>
		</id>
		
		<property name="value"/>
		<property name="error"/>
		<many-to-one
			name="reporter"
			class="org.rti.webcgh.domain.Reporter"
			lazy="false"
			column="reporter_id"
			update="false"
			insert="false"
		/>
		
	</class>

</hibernate-mapping>
