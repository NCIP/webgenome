<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="org.rti.webcgh.domain.BioAssay" table="BIO_ASSAY_2">
	
		<id name="id" column="ID">
		    <generator class="increment"/>
		</id>
		
		<property name="bioAssayDataId" column="bio_assay_data_id"/>
		<property name="name"/>
		<many-to-one
			name="organism"
			class="org.rti.webcgh.domain.Organism"
			lazy="false"
			column="organism_id"
		/>
		
	</class>

</hibernate-mapping>
