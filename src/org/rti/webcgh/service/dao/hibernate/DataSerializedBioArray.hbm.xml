<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping>

	<class name="org.rti.webcgh.domain.DataSerializedBioAssay" table="BIO_ASSAY">
	
		<id name="id" column="ID">
		    <generator class="increment"/>
		</id>
		
		<property name="name"/>
		<many-to-one
			name="organism"
			column="organism_id"
			class="org.rti.webcgh.domain.Organism"
			not-null="true"
			lazy="false"
		/>
		<many-to-one
			name="experiment"
			column="experiment_id"
			class="org.rti.webcgh.domain.Experiment"
			not-null="true"
			lazy="false"
			insert="false" update="false"
		/>
		
	</class>

</hibernate-mapping>
