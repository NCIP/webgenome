<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="org.rti.webcgh.array.persistent.impl.HibernatePersistentBioAssayData" table="bio_assay_data" lazy="false">
	
		<id name="id" column="ID">
		    <generator class="increment"/>
		</id>
		
		<bag name="arrayData" lazy="false" cascade="all">
			<key column="bio_assay_data_id"/>
			<one-to-many class="org.rti.webcgh.array.persistent.impl.HibernatePersistentArrayDatum"/>
		</bag>
		
	</class>

</hibernate-mapping>
