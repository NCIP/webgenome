<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="org.rti.webcgh.array.persistent.impl.HibernatePersistentBioAssay" table="BIO_ASSAY" lazy="false">
	
		<id name="id" column="ID">
		    <generator class="increment"/>
		</id>
		
		<property name="name"/>
		<property name="description"/>
		<property name="bioAssayDataId" column="bio_assay_data_id"/>
		<property name="binnedDataId" column="binned_data_id"/>
		<property name="databaseName" column="database_name"/>
		
	</class>

</hibernate-mapping>
