<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="org.rti.webcgh.array.persistent.impl.HibernatePersistentChromosomeBin" table="CHROMOSOME_BIN">
	
		<id name="id" column="ID">
		    <generator class="increment"/>
		</id>
		
		<property name="bin"/>
		<property name="value"/>
		<property name="chromosomeNum" column="chromosome_num"/>
	
	</class>

</hibernate-mapping>
