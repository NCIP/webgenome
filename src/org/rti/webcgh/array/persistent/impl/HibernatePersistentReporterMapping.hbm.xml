<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="org.rti.webcgh.array.persistent.impl.HibernatePersistentReporterMapping" table="REPORTER_MAPPING">
	
		<id name="id" column="ID">
		    <generator class="increment"/>
		</id>
		
		<many-to-one name="reporter" class="org.rti.webcgh.array.persistent.impl.HibernatePersistentReporter" lazy="false" column="reporter_id"/>
		<many-to-one name="genomeLocation" class="org.rti.webcgh.array.persistent.impl.HibernatePersistentGenomeLocation" lazy="false" column="genome_location_id" cascade="all"/>
	
	</class>

</hibernate-mapping>