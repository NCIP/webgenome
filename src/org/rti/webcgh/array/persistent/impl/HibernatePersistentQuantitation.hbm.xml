<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="org.rti.webcgh.array.persistent.impl.HibernatePersistentQuantitation" table="QUANTITATION">
	
		<id name="id" column="ID">
		    <generator class="increment"/>
		</id>
		
		<property name="value"/>
		
		<many-to-one name="quantitationType" class="org.rti.webcgh.array.persistent.impl.HibernatePersistentQuantitationType" lazy="false" column="quantitation_type_id"/>
	
	</class>

</hibernate-mapping>
