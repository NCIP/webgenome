<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="org.rti.webcgh.array.persistent.impl.HibernatePersistentArrayDatum" table="ARRAY_DATUM" lazy="false">
	
		<id name="id" column="ID">
		    <generator class="increment"/>
		</id>
		
		<many-to-one name="quantitation" class="org.rti.webcgh.array.persistent.impl.HibernatePersistentQuantitation" lazy="false" column="quantitation_id" cascade="all"/>
		<many-to-one name="reporter" class="org.rti.webcgh.array.persistent.impl.HibernatePersistentReporter" lazy="false" column="reporter_id"/>
	
	</class>

</hibernate-mapping>
