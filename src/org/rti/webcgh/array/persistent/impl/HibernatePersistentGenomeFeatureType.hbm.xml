<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="org.rti.webcgh.array.persistent.impl.HibernatePersistentGenomeFeatureType" table="GENOME_FEATURE_TYPE">
	
		<id name="id" column="ID">
		    <generator class="increment"/>
		</id>
		
		<property name="name"/>
		<property name="representsGene" column="represents_gene" type="yes_no"/>
	
	</class>

</hibernate-mapping>