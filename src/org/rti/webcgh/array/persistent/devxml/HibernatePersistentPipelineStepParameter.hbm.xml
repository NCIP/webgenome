<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class
    	name="org.rti.webcgh.array.persistent.impl.HibernatePersistentPipelineStepParameter" 
    	table="PIPELINE_STEP_PARAMETER"
    	lazy="false">
        <id name="id">
            <generator class="increment"/>
        </id>
        <property name="paramName" column="PARAM_NAME"/>
        <property name="value"/>
    </class>

</hibernate-mapping>