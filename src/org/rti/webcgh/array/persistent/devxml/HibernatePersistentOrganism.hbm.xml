<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="org.rti.webcgh.array.persistent.impl.HibernatePersistentOrganism" table="ORGANISM">
	
		<id name="id" column="ID">
		    <generator class="increment"/>
		</id>
		
		<property name="genus"/>
		<property name="species"/>
	
	</class>

</hibernate-mapping>