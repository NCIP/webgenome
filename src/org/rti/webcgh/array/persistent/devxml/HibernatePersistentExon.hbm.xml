<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="org.rti.webcgh.array.persistent.impl.HibernatePersistentExon" table="EXON">
	
		<id name="id" column="ID">
		    <generator class="increment"/>
		</id>
		
		<property name="start" column="chrom_start"/>
		<property name="end" column="chrom_end"/>
	
	</class>

</hibernate-mapping>