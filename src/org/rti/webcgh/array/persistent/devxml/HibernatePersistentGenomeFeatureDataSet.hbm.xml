<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="org.rti.webcgh.array.persistent.impl.HibernatePersistentGenomeFeatureDataSet" table="GENOME_FEATURE_DATA_SET">
	
		<id name="id" column="ID">
		    <generator class="increment"/>
		</id>
		
		<property name="uploadDate" column="upload_date"/>
		
		<many-to-one name="genomeFeatureType" class="org.rti.webcgh.array.persistent.impl.HibernatePersistentGenomeFeatureType" lazy="false" column="genome_feature_type_id"/>
		<many-to-one name="genomeAssembly" class="org.rti.webcgh.array.persistent.impl.HibernatePersistentGenomeAssembly" lazy="false" column="genome_assembly_id"/>
	
	</class>

</hibernate-mapping>