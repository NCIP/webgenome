<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>

<class 
    name="org.rti.webcgh.standalone.dao.hibernate.ProcessingJobStatus" 
    table="PROCESSING_JOB_STATUS"
>

    <id
        name="statusId"
        type="long"
        column="STATUS_ID"
    >
        <generator class="sequence">
            <param name="sequence">SQ_PROCESSING_JOB_STATUS</param>
        </generator>
    </id>

    <property
        name="status"
        type="java.lang.String"
        column="STATUS"
        not-null="true"
        length="50"
    />
    <property
        name="datetime"
        type="java.sql.Timestamp"
        column="DATETIME"
        length="11"
    />
    <property
        name="createdDt"
        type="java.sql.Date"
        column="CREATED_DT"
        length="7"
    />
    <property
        name="modifiedDt"
        type="java.sql.Date"
        column="MODIFIED_DT"
        length="7"
    />

    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to ProcessingJob -->
    <many-to-one
        name="processingJob"
        class="org.rti.webcgh.standalone.dao.hibernate.ProcessingJob"
        not-null="true"
    >
        <column name="JOB_ID" />
    </many-to-one>

</class>
</hibernate-mapping>
