<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping>

	<class name="org.rti.webgenome.domain.Cytoband" table="CYTOBAND">
	
		<id name="id" column="ID">
		    <generator class="increment"/>
		</id>
		
		<property name="name"/>
		<property name="start" column="start_loc"/>
		<property name="end" column="end_loc"/>
		<property name="stain"/>
		
	</class>

</hibernate-mapping>