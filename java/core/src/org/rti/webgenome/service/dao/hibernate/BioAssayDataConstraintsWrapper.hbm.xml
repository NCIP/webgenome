<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping>

	<class name="org.rti.webgenome.domain.Experiment$BioAssayDataConstraintsWrapper"
		table="bioassay_data_constraints">
		<id name="id" column="ID">
		    <generator class="increment"/>
		</id>
		<property name="chromosome"/>
		<property name="startPosition" column="start_point"/>
		<property name="endPosition" column="end_point"/>
		<property name="quantitationType" column="quant_type"/>
	</class>

</hibernate-mapping>
